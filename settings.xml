<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      https://maven.apache.org/xsd/settings-1.0.0.xsd">  <proxies>
   <proxy>
      <id>oa-http-proxy</id>
      <active>true</active>
      <protocol>http</protocol>
      <host>webproxy.openanalytics.eu</host>
      <port>8080</port>
      <nonProxyHosts>localhost|127.0.0.0|127.0.0.1|openanalytics.eu</nonProxyHosts>
    </proxy>
   <proxy>
      <id>oa-https-proxy</id>
      <active>true</active>
      <protocol>https</protocol>
      <host>webproxy.openanalytics.eu</host>
      <port>8080</port>
      <nonProxyHosts>localhost|127.0.0.0|127.0.0.1|openanalytics.eu</nonProxyHosts>
    </proxy>
  </proxies>
</settings>